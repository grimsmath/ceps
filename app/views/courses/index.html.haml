.col-md-12
  .panel.panel-info
    .panel-heading.clearfix
      %h4.panel-title.pull-left
        Courses
      .pull-right
        = link_to new_course_path, class: 'btn btn-primary btn-xs pull-right' do
          %i.fa.fa-plus
          New Course

    .panel-body
      #courses
        = render 'courses/partials/table'

    .panel-footer
      = link_to 'Manage Home', manage_path, class: 'btn btn-success'